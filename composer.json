{
  "name": "babymarkt/cron-bundle",
  "description": "A simple Symfony cron bundle to edit the system crontab.",
  "type": "symfony-bundle",
  "keywords": [
    "Cron",
    "Crontab",
    "Symfony2"
  ],
  "homepage": "https://github.com/Baby-Markt/cron-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Niklas Funke",
      "email": "funke@babymarkt.de"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "symfony/framework-bundle": "^2.8 || ^3.0",
    "symfony/console": "^2.8 || ^3.0",
    "symfony/yaml": "^2.8 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "mikey179/vfsstream": "~v1.6"
  },
  "autoload": {
    "psr-4": {
      "BabymarktExt\\CronBundle\\": ""
    }
  },
  "config": {
    "platform": {
      "php": "7.3"
    }
  }
}
